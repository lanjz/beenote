    "startTs:end": "tsc && cross-env DEV=1 JUSTNODE=1  node --inspect dist/app.js",
    "startTs": "tsc && cross-env DEV=1 node --inspect dist/app.js",
    "inspect": "ts-node --inspect ./server/app.ts"
